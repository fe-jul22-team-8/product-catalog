{
  "name": "server",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "build": "swc src -d dist",
    "start": "node dist/server.js",
    "format": "prettier --write . --ignore-path .gitignore",
    "lint": "eslint . --ignore-path .gitignore --fix",
    "fix-style": "npm run format && npm run lint -- --fix"
  },
  "lint-staged": {
    "*.js": "npm run lint"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "lodash": "^4.17.21",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.11"
  },
  "devDependencies": {
    "@swc-node/register": "^1.5.4",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.11",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.7",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "eslint": "^8.26.0",
    "nodemon": "^2.0.20",
    "typescript": "^4.8.4",
    "prettier": "2.7.1"
  }
}
